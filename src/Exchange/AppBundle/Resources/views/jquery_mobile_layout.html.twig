{% extends '::base.html.twig' %}

{% block meta %}
    {{ parent() }}
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no, maximum-scale=1" />
{% endblock meta %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/bmatznerjquery/js/jquery.min.js') }}"></script>
